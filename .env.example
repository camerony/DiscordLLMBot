# Discord bot token (required)
DISCORD_TOKEN=your_bot_token_here

# LLM API endpoint (OpenAI-compatible)
LLM_URL=http://llm.home.cameron.in:8080/v1/chat/completions

# Model name to use
LLM_MODEL=mlx-community/Qwen3-30B-A3B-4bit

# Manual channel pairs (optional, format: id1:id2,id3:id4)
# Overrides auto-detection for specific channels
CHANNEL_PAIRS=

# Enable debug logging (optional, default: false)
DEBUG_MODE=false
